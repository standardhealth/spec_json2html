{{#each children as |child| }}
  {{#if child}}
    <tr>
      {{>shr_field child isChoice=isChoice val=val isFromBase=false}}
    </tr>
  {{/if}}
{{/each}}
{{#if basedOn}}
	{{#each basedOn as |basedon|}}
		{{#eachelementchild @root.shr_v4 basedon.namespace basedon.label ../children getparent=true }}
			<tr>
			  {{>shr_field this isChoice=isChoice val=val isFromBase=true base=@base}}
			</tr>
		{{/eachelementchild}}
	{{/each}}
{{/if}}